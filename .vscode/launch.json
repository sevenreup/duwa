    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}\\src\\cmd\\duwa\\duwa.go",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "args": [
                "-f",
                "${workspaceRoot}\\examples\\games\\tictactoe.duwa"
            ]
        },
        {
            "name": "Wasm",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}/src/cmd/wasm/duwa.go",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                "-f",
                "${workspaceRoot}\\examples\\games\\tictactoe.duwa"
            ]
        }
    ]
}